import math
import os
import random
import re
import sys


def plusMinus(arr):
    pcount = ncount = zcount = 0
    for i in arr:
        if i>0:
            pcount += 1
        if i<0:
            ncount += 1
        if i==0:
            zcount += 1
    print(pcount/len(arr))
    print(ncount/len(arr))
    print(zcount/len(arr))
    
                    
if __name__ == '__main__':
    n = int(input().strip())

    arr = list(map(int, input().rstrip().split()))

    plusMinus(arr)
